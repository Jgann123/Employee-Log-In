<h1 id="createA">Create Account.</h1>
<div class="flex-box-container-1">
<form id="signup" name="signup" method="POST" action="/users/signup">
    <div>
        <label id="firstNameL" for="name">First Name: </label>
        <input id="firstName" type="text" name="FirstName" class="form-control" placeholder="FirstName*" required>
    </div>
    <div>
        <label id="lastNameL" for="name">Last Name: </label>
        <input id="lastName" type="text" name="LastName" class="form-control" placeholder="lastName*"  required>
    </div>
    <div>
        <label id="emailL"  for="name">Email: </label>
        <input id="email" type="text" name="Email" class="form-control" placeholder="Email*"  required>
    </div>
    <div>
        <label id="userNameL" for="name">Username: </label>
        <input id="userName" type="text" name="UserName" class="form-control" placeholder="UserName*"  required>
    </div>
    <div>
        <label id="passwordL" for="name">Password: </label>
        <input id="password" type="password" name="Password" class="form-control" placeholder="Password*"  required>
    </div>
    <div>
        <button id="submit1" type="submit">Submit</button>
    </div>
</form>
</div>

